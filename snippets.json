{
    "button component": {
        "prefix": "p6-button-component",
        "body": "<p-button type label=\"${1:label}\" icon iconPos disabled style styleClass=\"${2:}\" (onClick)=\"${3:}\" onFocus onBlur></p-button>$0",
        "description": "button-component",
        "scope": ""
    },
    "button directive": {
        "prefix": "p6-button-directive",
        "body": "<button pButton icon iconPos type=\"button\" label=\"${1:label}\" class=\"${2:}\" (click)=\"${3:}\"></button>$0",
        "description": "button-directive",
        "scope": ""
    },
    "button splitButton": {
        "prefix": "p6-button-splitButton",
        "body": "<p-splitButton label=\"${1:label}\" icon=\"${2:icon}\" (onClick)=\"${3:click()}\" [model]=\"${4:items}\" styleClass=\"${5:class}\"></p-splitButton>$0",
        "description": "button-splitButton",
        "scope": ""
    },
    "input inputGroupAddons": {
        "prefix": "p6-input-inputGroup-addons",
        "body": "<div class=\"ui-g-12 ui-md-4\">\n\t<div class=\"ui-inputgroup\">\n\t\t<span class=\"ui-inputgroup-addon\">${1:$}</span>\n\t\t<input type=\"text\" pInputText placeholder=\"${2:placeholder}\">\n\t\t<span class=\"ui-inputgroup-addon\">${3:.00}</span>\n\t</div>\n</div>$0",
        "description": "input-inputGroup-addons",
        "scope": ""
    },
    "input inputGroupMultipleAddons": {
        "prefix": "p6-input-inputGroup-multiple-addons",
        "body": "<div class=\"ui-g\">\n\t<div class=\"ui-g-12\">\n\t\t<div class=\"ui-inputgroup\">\n\t\t\t<span class=\"ui-inputgroup-addon\"><i class=\"fa fa-credit-card\"></i></span>\n\t\t\t<span class=\"ui-inputgroup-addon\"><i class=\"fa fa-cc-visa\"></i></span>\n\t\t\t<input type=\"text\" pInputText placeholder=\"Price\">\n\t\t\t<span class=\"ui-inputgroup-addon\">$</span>\n\t\t\t<span class=\"ui-inputgroup-addon\">.00</span>\n\t\t</div>\n\t</div>\n</div>$0",
        "description": "input-inputGroup-multiple-addons",
        "scope": ""
    },
    "input inputGroupButtonAddons": {
        "prefix": "p6-input-inputGroup-button-addons",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<div class=\"ui-inputgroup\">\n\t\t\t<button pButton type=\"button\" icon=\"pi pi-check\" class=\"ui-button-success\"></button>\n\t\t\t<input type=\"text\" pInputText placeholder=\"Vote\">\n\t\t\t<button pButton type=\"button\" icon=\"pi pi-close\" class=\"ui-button-danger\"></button>\n\t\t</div>\n\t</div>\n</div>$0",
        "description": "input-inputGroup-button-addons",
        "scope": ""
    },
    "input inputGroupCheckbox": {
        "prefix": "p6-input-inputGroup-checkbox",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<div class=\"ui-inputgroup\">\n\t\t\t<span class=\"ui-inputgroup-addon\"><p-checkbox></p-checkbox></span>\n\t\t\t<input type=\"text\" pInputText placeholder=\"Username\">\n\t\t</div>\n\t</div>\n</div>$0",
        "description": "input-inputGroup-checkbox",
        "scope": ""
    },
    "input calendarBasic": {
        "prefix": "p6-input-calendar-basic",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<p-calendar [(ngModel)]=\"${1:date1}\"></p-calendar>\n\t</div>\n</div>$0",
        "description": "input-calendar-basic",
        "scope": ""
    },
    "input calendarIcon": {
        "prefix": "p6-input-calendar-icon",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<p-calendar [(ngModel)]=\"${1:date1}\" [showIcon]=\"true\"></p-calendar>\n\t</div>\n</div>$0",
        "description": "input-calendar-icon",
        "scope": ""
    },
    "input calendarNavigators": {
        "prefix": "p6-input-calendar-navigators",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<p-calendar [(ngModel)]=\"${1:date1}\" [monthNavigator]=\"true\" [yearNavigator]=\"true\" yearRange=\"2000: 2030\"></p-calendar>\n\t</div>\n</div>$0",
        "description": "input-calendar-navigators",
        "scope": ""
    },
    "input calendarTime": {
        "prefix": "p6-input-calendar-time",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<p-calendar [(ngModel)]=\"${1:date1}\" [showTime]=\"true\"></p-calendar>\n\t</div>\n</div>$0",
        "description": "input-calendar-time",
        "scope": ""
    },
    "input calendarTimeOnly": {
        "prefix": "p6-input-calendar-time-only",
        "body": "<div class=\"ui-g ui-fluid\">\n\t<div class=\"ui-g-12 ui-md-4\">\n\t\t<p-calendar [(ngModel)]=\"${1:date1}\" [timeOnly]=\"true\"></p-calendar>\n\t</div>\n</div>$0",
        "description": "input-calendar-time-only",
        "scope": ""
    },
    "data table": {
        "prefix": "p6-data-table-basic",
        "body": "<p-table [value]=\"${1:array}\">\n\t<ng-template pTemplate=\"header\">\n\t\t<tr>\n\t\t\t<th>Column1</th>\n\t\t\t<th>Column2</th>\n\t\t\t<th>Column3</th>\n\t\t\t<th>Column4</th>\n\t\t</tr>\n\t</ng-template>\n\t<ng-template pTemplate=\"body\" let-${2:}>\n\t\t<tr>\n\t\t\t<td></td>\n\t\t\t<td></td>\n\t\t\t<td></td>\n\t\t\t<td></td>\n\t\t</tr>\n\t</ng-template>\n</p-table>$0",
        "description": "data-table-basic",
        "scope": ""
    },
    "panel panelBasic": {
        "prefix": "p6-panel-panel-basic",
        "body": "<p-panel header=\"Title\">\n\tContent\n</p-panel>$0",
        "description": "panel-panel-basic",
        "scope": ""
    },
    "panel panelToggleable": {
        "prefix": "p6-panel-panel-toggleable",
        "body": "<p-panel header=\"Title\" [toggleable]=\"true\">\n\tContent\n</p-panel>$0",
        "description": "panel-panel-toggleable",
        "scope": ""
    },
    "panel panelHeaderAndFooter": {
        "prefix": "p6-panel-panel-header-and-footer",
        "body": "<p-panel>\n\t<p-header>\n\t\tHeader content here\n\t</p-header>\n\tBody Content\n\t<p-footer>\n\t\tFooter content here\n\t</p-footer>\n</p-panel>$0",
        "description": "panel-panel-header-and-footer",
        "scope": ""
    },
    "overlay dialogBasic": {
        "prefix": "p6-overlay-dialog-basic",
        "body": "<p-dialog header=\"Title\" [(visible)]=\"${1:display}\">\n\tContent\n</p-dialog>$0",
        "description": "overlay-dialog-basic",
        "scope": ""
    },
    "overlay dialogWidth": {
        "prefix": "p6-overlay-dialog-width",
        "body": "<p-dialog header=\"Title\" [(visible)]=\"${1:display}\" [width]=\"300\">\n\tContent\n</p-dialog>$0",
        "description": "overlay-dialog-width",
        "scope": ""
    },
    "overlay dialogHeaderAndFooter": {
        "prefix": "p6-overlay-dialog-header-and-footer",
        "body": "<p-dialog [(visible)]=\"${1:display}\">\n\t<p-header>\n\t\tHeader content here\n\t</p-header>\n\tContent\n\t<p-footer>\n\t\t//buttons\n\t</p-footer>\n</p-dialog>$0",
        "description": "overlay-dialog-header-and-footer",
        "scope": ""
    }
}
